---
import CesiumFrame from "../components/cesiumFrame.html";
import MatterportFrame from "../components/MatterportFrame.astro";

import Sidenav from "../components/Sidenav.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
---

<BaseLayout title="ICT Explorer + Room Finder">
  <!-- Include the CesiumJS JavaScript and CSS files -->
  <script
    slot="head"
    src="/Cesium.js"
  ></script>
  <link
    slot="head"
    href="/widgets.css"
    rel="stylesheet"
  />

  <Sidenav />
  <MatterportFrame />
  <CesiumFrame />
</BaseLayout>
